alias ls='ls --color=auto --group-directories-first --time-style=long-iso'
alias l='ls -CF'
alias ll='ls -lF '
alias la='ls -AF'
alias lla='ls -AlF'

alias dot='/usr/bin/git --git-dir=$HOME/dotfiles/ --work-tree=$HOME'

alias tops="rank_grouped_processes_by_mem | less"
alias xvp="xargs -d '\n' -o vim -p"

