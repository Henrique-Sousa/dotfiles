[diff]
	tool = vimdiff
[difftool]
	prompt = false
[core]
	editor = vim
  pager = cat
	excludesfile = ~/.excludesfile
[alias]
  s = status
	lo = log --oneline -n 30
  a = add
  ap = add --patch
  c = commit
  cm = commit -m 
  d = diff
  dc = diff --cached
  dt = difftool
  dtc = difftool --cached
  unmodify = checkout --
  unstage = reset HEAD --
  untrack = rm --cached
